{
    "presets": [
        [
            "latest",
            {
                "es2015": {
                    "modules": false
                }
            }
        ],
        "stage-2"
    ],
    "ignore": [
        "src/libs/**"
    ],
    "plugins": [
        "transform-runtime",
        [
            "component",
            [
                {
                    "libraryName": "element-ui",
                    "styleLibraryName": "~theme"
                }
            ]
        ]
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                "latest",
                "stage-2"
            ],
            "plugins": [
                "istanbul"
            ]
        }
    }
}
